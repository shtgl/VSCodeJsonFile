{
  "workbench.settings.applyToAllProfiles": [
    "editor.tabSize"
  ],
  "chat.editor.wordWrap": "on",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "files.hotExit": "off",
  "task.quickOpen.history": 0,
  "telemetry.feedback.enabled": false,
  "editor.fontFamily": "Consolas",
  "editor.tabSize": 2,
  "editor.rulers": [120],
  "editor.folding": true,
  "editor.cursorWidth": 2,
  "editor.lineHeight": 1.4,
  "editor.lineNumbers": "on",
  "editor.glyphMargin": true,
  "editor.fontLigatures": true,
  "editor.wordWrapColumn": 120,
  "editor.mouseWheelZoom": true,
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "smooth",
  "editor.wordWrap": "wordWrapColumn",
  "editor.inlineSuggest.enabled": false,
  "editor.tokenColorCustomizations": {},
  "editor.suggestOnTriggerCharacters": false,
  "editor.language.colorizedBracketPairs": [],
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.stickyScroll.maxLineCount": 3,
  "git.confirmSync": false,
  "git.decorations.enabled": true,
  "git.autoRepositoryDetection": false,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.renameSuggestions.triggerAutomatically": false,
  "python.createEnvironment.trigger": "off",
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.confirmOnExit": "always",
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.cursorStyleInactive": "none",
  "workbench.editor.highlightModifiedTabs": true,
  "window.restoreWindows": "none",
  "window.title": "${dirty}${folderName}${separator}${activeEditorShort} - VSCode",
  "workbench.colorCustomizations": {
    "editor.background": "#282629",
    "editor.lineHighlightBackground": "#2c2a2d",
    "editorLineNumber.activeForeground": "#3b9f30",
    "tab.activeBackground": "#312f31",
    "tab.inactiveBackground": "#2c2a2d",
    "tab.activeForeground": "#F3F3F3",
    "tab.activeModifiedBorder": "#323232",
    "sideBar.background": "#2c2a2d",
    "sideBar.foreground": "#a29e9e",
    "list.activeSelectionBackground": "#312f31",
    "list.inactiveSelectionBackground": "#312f31",
    "list.inactiveSelectionForeground": "#F3F3F3",
    "list.hoverForeground": "#F3F3F3",
    "list.hoverBackground": "#312f31",
    "activityBar.background": "#312f31",
    "activityBar.activeBorder": "#9f4a30",
    "activityBar.foreground": "#a29e9e",
    "titleBar.activeBackground": "#292925",
    "titleBar.activeForeground": "#a29e9e",
    "editorGroupHeader.tabsBackground": "#2c2a2d",
    "statusBar.background": "#312f31",
    "statusBar.foreground": "#F3F3F3",
    "button.background": "#303d9f",
    "menu.foreground": "#a29e9e",
    "menu.selectionForeground": "#F3F3F3",
    "menu.selectionBackground": "#2c2a2d"
  },
  "window.zoomPerWindow": false,
  "workbench.reduceMotion": "on",
  "workbench.list.scrollByPage": true,
  "workbench.commandPalette.history": 10,
  "workbench.activityBar.location": "top",
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.tree.stickyScrollMaxItemCount": 3,
  "workbench.editor.tabSizingFixedMinWidth": 80,
  "workbench.editor.tabSizingFixedMaxWidth": 120,
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.editor.focusRecentEditorAfterClose": false,
  "workbench.editor.historyBasedLanguageDetection": false,
  "search.quickOpen.includeHistory": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/env": true,
    "**/venv": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/env/**": true,
    "**/venv/**": true,
    "env-*": true
  }
}
